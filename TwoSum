public class Solution {
    public int[] TwoSum(int[] nums, int target) {
        
        int[] a = new int[2];
        for(int i=0; i<nums.Length; i++){
            for(int j=0; j<nums.Length&&j!=i; j++){
                int sum = nums[i]+nums[j];
                if(sum == target){
                    a[0]=j;
                    a[1]=i;
                }
            }
        }
        return a;
    }
}
